mode "normal" {
    # Normal mode navigation bindings here
    bindsym left      focus left
    bindsym down      focus down
    bindsym up      focus up
    bindsym right      focus right

    # Enter insert mode
    bindsym Super_L mode "insert"
}
mode "insert" {
    # Insert mode bindings here

    # Return to normal mode
    bindsym Super_L mode "normal"
}
# Start in normal mode
exec_always swaymsg mode normal
